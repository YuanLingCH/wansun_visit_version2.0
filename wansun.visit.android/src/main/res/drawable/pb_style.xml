<?xml version="1.0" encoding="UTF-8"?>
<!-- progress版本更新进度条 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 设置背景色图像资源 -->
    <item
        android:id="@android:id/background"
       android:drawable="@drawable/img_pb_bg"/>
    <!-- 设置第一级进度条颜色图像资源 -->
    <item
        android:id="@android:id/progress"
        android:left="-10.5dp"
        android:right="-10dp"
        android:top="0.5dp">
        <clip>
            <nine-patch android:src="@drawable/img_progress" />
        </clip>
    </item>

</layer-list>